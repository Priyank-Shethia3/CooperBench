+import pytest
+import tiktoken
+
+def test_token_filtering():
+    # Create a simple encoding for testing
+    enc = tiktoken.get_encoding("cl100k_base")
+ 
+    # Test basic filtering
+    text = "I love you"
+    tokens = enc.encode(text, filter_tokens=[])
+    assert len(tokens) == 3
+ 
+    text = "I love you"
+    tokens = enc.encode(text, filter_tokens=[1])
+    assert len(tokens) == 3
+ 
+    text = "I love you"
+    tokens = enc.encode(text, filter_tokens=[40])
+    assert len(tokens) == 2
+    assert 40 not in tokens
+ 
+    text = "I love you"
+    tokens = enc.encode(text, filter_tokens=[40, 499])
+    assert len(tokens) == 1
+    assert tokens == [3021]
