+# IsURL Tests
+@pytest.mark.parametrize(
+    'url,dirty',
+    [
+        ('https://example.com', IsURL()),
+        ('https://example.com', IsURL),
+        ('http://example.com', IsURL()),
+        ('ftp://example.com', IsURL()),
+        ('https://example.com/path', IsURL()),
+        ('https://example.com:8080', IsURL()),
+        ('https://subdomain.example.com', IsURL()),
+        ('https://example.com/path?query=value', IsURL()),
+        ('https://example.com/path#fragment', IsURL()),
+        ('https://example.com/path?query=value#fragment', IsURL()),
+        ('https://example.com', IsURL(protocol='https')),
+        ('http://example.com', IsURL(protocol='http')),
+        ('https://example.com', IsURL(protocol=['http', 'https'])),
+        ('http://example.com', IsURL(protocol=['http', 'https'])),
+        ('ftp://example.com', IsURL(protocol=['ftp', 'sftp'])),
+        ('https://example.com', IsURL(domain='example.com')),
+        ('http://example.com', IsURL(domain='example.com')),
+        ('https://example.com', IsURL(protocol='https', domain='example.com')),
+        ('https://example.com/path', IsURL(protocol='https', domain='example.com')),
+    ],
+)
+def test_is_url_true(url, dirty):
+    assert url == dirty
+
+
+@pytest.mark.parametrize(
+    'url,dirty',
+    [
+        ('not-a-url', IsURL()),
+        ('', IsURL()),
+        ('example.com', IsURL()),  # Missing protocol
+        ('://example.com', IsURL()),  # Missing scheme
+        ('https://', IsURL()),  # Missing netloc
+        ('https://example.com', IsURL(protocol='http')),
+        ('http://example.com', IsURL(protocol='https')),
+        ('ftp://example.com', IsURL(protocol='https')),
+        ('https://example.com', IsURL(protocol=['http', 'ftp'])),
+        ('https://example.com', IsURL(domain='other.com')),
+        ('https://subdomain.example.com', IsURL(domain='example.com')),
+        ('https://example.com', IsURL(protocol='http', domain='example.com')),
+        ('http://example.com', IsURL(protocol='https', domain='example.com')),
+        ('https://other.com', IsURL(protocol='https', domain='example.com')),
+        (123, IsURL()),
+        ([1, 2, 3], IsURL()),
+        (None, IsURL()),
+    ],
+)
+def test_is_url_false(url, dirty):
+    assert url != dirty
+
+
+def test_is_url_repr():
+    is_url = IsURL()
+    with pytest.raises(AssertionError):
+        assert 'not-a-url' == is_url
+    assert str(is_url) == 'IsURL(*)'
+
+
+def test_is_url_protocol_repr():
+    is_url = IsURL(protocol='https')
+    with pytest.raises(AssertionError):
+        assert 'http://example.com' == is_url
+    assert str(is_url) == "IsURL('https')"
+
+
+def test_is_url_domain_repr():
+    is_url = IsURL(domain='example.com')
+    with pytest.raises(AssertionError):
+        assert 'https://other.com' == is_url
+    assert str(is_url) == "IsURL('example.com')"
+
+
+def test_is_url_protocol_domain_repr():
+    is_url = IsURL(protocol='https', domain='example.com')
+    with pytest.raises(AssertionError):
+        assert 'http://other.com' == is_url
+    assert str(is_url) == "IsURL('https', 'example.com')"
+
+
+def test_is_url_protocol_list():
+    assert 'https://example.com' == IsURL(protocol=['http', 'https'])
+    assert 'http://example.com' == IsURL(protocol=['http', 'https'])
+    assert 'ftp://example.com' != IsURL(protocol=['http', 'https'])
+
+
+def test_is_url_edge_cases():
+    # Test with port numbers
+    assert 'https://example.com:8080' == IsURL(domain='example.com:8080')
+    assert 'https://example.com:8080' != IsURL(domain='example.com')
+ 
+    # Test with complex paths
+    assert 'https://example.com/very/long/path/with/many/segments' == IsURL()
+ 
+    # Test with query parameters and fragments
+    assert 'https://example.com?a=1&b=2' == IsURL()
+    assert 'https://example.com#section' == IsURL()
+    assert 'https://example.com/path?query=value#fragment' == IsURL()
+
+
+def test_is_url_performance():
+    # Test with a large number of URLs to ensure reasonable performance
+    urls = [f'https://example{i}.com' for i in range(100)]
+    validator = IsURL()
+    for url in urls:
+        assert url == validator

